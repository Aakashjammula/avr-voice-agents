{
  "name": "openai-sts",
  "version": "1.1.0",
  "author": "aakash jammula",
  "description": "OpenAI Speech-to-Speech service for Agent Voice Response",
  "main": "index.js",
  "scripts": {
    "start": "node .",
    "start:dev": "nodemon --inspect ./index.js",
    "dc:build": "docker build --platform=linux/amd64 -t aakashjammula/openai-sts:latest -t aakashjammula/openai-sts:${npm_package_version} .",
    "dc:push": "docker push aakashjammula/openai-sts:latest && docker push aakashjammula/openai-sts:${npm_package_version}"
  },
  "dependencies": {
    "axios": "^1.8.4",
    "dotenv": "^16.5.0",
    "express": "^5.1.0",
    "ws": "^8.18.2"
  },
  "devDependencies": {
    "nodemon": "^3.1.9"
  }
}